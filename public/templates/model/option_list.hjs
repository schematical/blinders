{{> _header }}

{{> _navbar }}
<div class="container">
    <div class="row">
        <div class="col-md-3">
            {{> _option_edit_form }}
        </div>
        <div class="col-md-8 col-md-offset-1">
            <table class="table">
                <tr>
                    
                        <th>
                            namespace
                        </th>
                    
                        <th>
                            name
                        </th>
                    
                        <th>
                            desc
                        </th>
                    
                        <th>
                            product
                        </th>
                    
                        <th>
                            carbs
                        </th>
                    
                        <th>
                            calories
                        </th>
                    
                        <th>
                            points
                        </th>
                    
                </tr>
                {{# options }}
                    <tr>
                        
                            <td>
                                {{ namespace }}
                            </td>
                        
                            <td>
                                {{ name }}
                            </td>
                        
                            <td>
                                {{ desc }}
                            </td>
                        
                            <td>
                                {{ product }}
                            </td>
                        
                            <td>
                                {{ carbs }}
                            </td>
                        
                            <td>
                                {{ calories }}
                            </td>
                        
                            <td>
                                {{ points }}
                            </td>
                        
                        
                        <td>
                            <a class='btn btn-default btn-sm' href='{{ uri }}'>
                                View
                            </a>
                        </td>
                        
                    </tr>
                {{/ options }}
            </table>
        </div>
    </div>
</div>
{{> _footer }}

